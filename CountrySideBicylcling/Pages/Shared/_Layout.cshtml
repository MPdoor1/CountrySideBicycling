<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CountrySideBicylcling</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/CountrySideBicylcling.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
    
            @*Navbar*@
            <nav class="nav" id="navbarContainer">
                <div class="container ">

                    @*Navbar Items*@
                    <ul class="nav justify-content-center flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-page="/Index">Home</a>
                        </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-page="/Index">Contact</a>
                    </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Brands</a>
                            <ul class="dropdown-menu fade-up">
                                <li><a class="dropdown-item" asp-page="/Music"> Music</a></li>
                                <li>
                                    <a class="dropdown-item" href="
https://discord.gg/6ZVudKeK2v">Discord</a>
                                </li>
                                <li>
                                    <a class="dropdown-item" href="https://www.twitch.tv/mpdoor
">Twitch</a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-page="/Youtube">Youtube</a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
    </header>
    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - CountrySideBicylcling - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>