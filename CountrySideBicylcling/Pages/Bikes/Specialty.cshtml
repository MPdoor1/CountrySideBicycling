@*RECUMBANT*@
<div id="scooterContainer">
    <div style="height:100px;"></div>
    <div class="headCont">
        <h3 class="head">Recumbant</h3>
    </div>
    <h5 class="sloganHeader">Sun Bikes</h5>
    <div style="height:25px;"></div>
